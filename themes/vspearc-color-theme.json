	"tokenColors": [
		{
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#FF0000"
			}
		},
		{
			"scope": [
				"meta.function",
				"storage.type.function",
				"string meta.method-call",
				"string variable.other",
				"variable.other.object",
